{
  "name": "deepseek-mcp-server",
  "version": "0.4.0",
  "mcpName": "io.github.DMontgomery40/deepseek",
  "description": "Model Context Protocol server for DeepSeek API endpoints with stdio and Streamable HTTP transports.",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "deepseek-mcp-server": "./build/index.js"
  },
  "files": [
    "build/**/*",
    "assets/favicon.ico",
    "assets/favicon-32x32.png",
    "assets/apple-touch-icon.png"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "test": "vitest run --config ./vitest.config.ts",
    "test:watch": "vitest --config ./vitest.config.ts",
    "test:live": "npm run build && node scripts/live-smoke.mjs",
    "test:remote": "npm run build && node scripts/remote-smoke.mjs",
    "prepublishOnly": "npm run build",
    "inspect": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "dotenv": "^16.6.1",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "author": "DMontgomery40",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DMontgomery40/deepseek-mcp-server"
  },
  "keywords": [
    "mcp",
    "deepseek",
    "ai",
    "llm"
  ]
}
